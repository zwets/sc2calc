#  This program is distributed in the hope that it will be useful,
#  but WITHOUT ANY WARRANTY; without even the implied warranty of
#  MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
#  GNU General Public License for more details.
#
#  You should have received a copy of the GNU General Public License
#  along with this program.  If not, see <http://www.gnu.org/licenses/>.
#
#  Home: https://github.com/zwets/sc2calc

BASE_DIR="$(dirname "$0")"
export LC_ALL="C"
shopt -s extglob globasciiranges

### CONSTANTS -------------------------------------------------------------------------------------

# Translate codon to aa
declare -rAu cod_aa=(
  aaa K aac N aag K aat N aca T acc T acg T act T aga R agc S agg R agt S ata I atc I atg M att I
  caa Q cac H cag Q cat H cca P ccc P ccg P cct P cga R cgc R cgg R cgt R cta L ctc L ctg L ctt L
  gaa E gac D gag E gat D gca A gcc A gcg A gct A gga G ggc G ggg G ggt G gta V gtc V gtg V gtt V
  taa * tac Y tag * tat Y tca S tcc S tcg S tct S tga * tgc C tgg W tgt C tta L ttc F ttg L ttt F)

# Start positions for all named regions
declare -rAi beg_pos=(
  5UTR       0 ORF1AB   265 ORF1A    265 NSP1     265 NSP2     805 NSP3    2719 NSP4    8554
  NSP5   10054 NSP6   10972 NSP7   11842 NSP8   12091 NSP9   12685 NSP10  13024 NSP11  13441
  NSP12  13441 NSP13  16236 NSP14  18039 NSP15  19620 NSP16  20658 S      21562 ORF3A  25392
  ORF3B  25764 E      26244 M      26522 ORF6   27201 ORF7A  27393 ORF7B  27755 ORF8   27893
  N      28273 ORF9A  28283 ORF9B  28733 ORF10  29557 3UTR   29674)

# End positions for all named regions
declare -rAi end_pos=(
  5UTR     265 ORF1AB 21555 ORF1A  13483 NSP1     805 NSP2    2719 NSP3    8554 NSP4   10054
  NSP5   10972 NSP6   11842 NSP7   12091 NSP8   12685 NSP9   13024 NSP10  13441 NSP11  13480
  NSP12  16236 NSP13  18039 NSP14  19620 NSP15  20658 NSP16  21552 S      25384 ORF3A  26220
  ORF3B  26220 E      26472 M      27191 ORF6   27387 ORF7A  27759 ORF7B  27887 ORF8   28259
  N      29533 ORF9A  28577 ORF9B  28955 ORF10  29674 3UTR   29903)

# Display names and order of the products in output
declare -r reg_names="5UTR orf1ab nsp1 nsp2 nsp3 nsp4 nsp5 nsp6 nsp7 nsp8 nsp9 nsp10 nsp11
                      nsp12 nsp13 nsp14 nsp15 nsp16 S ORF3a E M ORF6 ORF7a ORF8 N ORF10 3UTR"
declare -rau reg_ord=($reg_names)
declare -ra reg_name=($reg_names)

# The slippage position
declare -ri slip_pos=13467

# Regex constants for pattern matching
declare -r CDS='+([[:alnum:]])'
declare -r NUM='+([[:digit:]])' POS='+([[:digit:]])'
declare -r NT='[acgtACGT]'   NTS='+([acgtACGT])' NTO='*([acgtACGT])'
declare -r AA='[[:alpha:]*]' AAS='+([[:alpha:]*])' AAO='*([[:alpha:]*])'

# The genome and genome with slippage
declare -rl genome=""
declare -rl slip_genome="${genome:0:$((slip_pos+1))}${genome:$slip_pos}"

### FUNCTIONS -------------------------------------------------------------------------------------

err_msg() { echo "$(basename "$0"): $*" >&2; }

err_exit() { err_msg $*; exit 1; }

debug() { [ ! $DEBUG ] || echo "$(basename "$0"): $*" >&2; }

usage_exit() {
    printf '
Usage: %s [OPTIONS]

  Converts between nucleotide and amino acid coordinates on the SARS-CoV-2
  genome.  Reads from stdin, writes to stdout.

  OPTIONS
   -d,--debug  Outputs debug info to stderr
   -h,--help   Shows this information and exits

  Accepts input in a variety of formats.  See the README or just try it out.
  To read args from the cmdline instead of stdin use <<< args (in bash).

' "$(basename "$0")" &&
    exit ${1:-1}
}

### PARSE ARGS ------------------------------------------------------------------------------------

unset DEBUG
while [[ $# -gt 0 && "$1" =~ ^-.*$ ]]; do
    case $1 in
        -d|--debug)   DEBUG=1 ;;
        -h|--help)    usage_exit 0 ;;
        *) usage_exit ;;
    esac
    shift
done

(( ! $# )) || usage_exit

### PARSE INPUT LINE ------------------------------------------------------------------------------
#
# Implementation Notes
#  - all positions internally are C-like 0-based open ranges: [b,e)
#  - aa and genomic regions are upper case, nucleotides are lower case
#  - we parse all known CDS & product names but output only the accepted ones
#  - special handling for orf1ab and nsp12 due to the -1 slippage at 13467

parse_line() {
    declare -a args
    declare -g label=""
    declare -gi nt_pos=0 aa_pos=0 ref_len=0 alt_len=0
    declare -gl nt_ref="" nt_alt=""
    declare -gu aa_cds="" aa_ref="" aa_alt=""

    read -a args || return 1

    case ${#args[@]} in 0) return 0 ;; 2|4) label="${args[0]}" ;; esac
    case ${#args[@]} in

    1|2)
        a="${args[-1]}"
        debug "a = $a"
        case "$a" in

        # [nt:]del:12345:acg
        ?(nt:)del:$POS:$NTS)        r=${a/#*del:} nt_pos=${r/%:*} nt_ref=${r/#*:} ref_len=${#nt_ref} ;;
        # [nt:]del:12345:3
        ?(nt:)del:$POS:$NUM)        r=${a/#*del:} nt_pos=${r/%:*} ref_len=${r/#*:} ;;
        # [nt:]12345:3-
        ?(nt:)$POS:$NUM-)           r=${a/#nt:}   nt_pos=${r/%:*} r=${r%-} ref_len=${r/#*:} ;;

        # [aa:]del:S:345:QR
        ?(aa:)del:$CDS:$POS:$AAS)   r=${a/#*del:} aa_cds=${r/%:*} r=${r/#$CDS:} aa_pos=${r/%:*} aa_ref=${r/#*:} ref_len=${#aa_ref} ;;
        # [aa:]del:S:345:3
        ?(aa:)del:$CDS:$POS:$NUM)   r=${a/#*del:} aa_cds=${r/%:*} r=${r/#$CDS:} aa_pos=${r/%:*} ref_len=${r/#*:} ;;
        # [aa:]:S:345:1-
        ?(aa:)$CDS:$POS:$NUM-)      r=${a/#aa:}   aa_cds=${r/%:*} r=${r/#$CDS:} aa_pos=${r/%:*} r=${r%-} ref_len=${r/#*:} ;;

        # [nt:]ins:12345:acg
        ?(nt:)ins:$POS:$NTS)        r=${a/#*ins:}
                                    nt_pos=${r/%:*} nt_alt=${r/#*:} alt_len=${#nt_alt} ;;
        # [aa:]ins:12345:KL
        ?(aa:)ins:$CDS:$POS:$AAS)   r=${a/#*ins:} aa_cds=${r/%:*} r=${r/#$CDS:}r
                                    aa_pos=${r/%:*} aa_alt=${r/#*:} alt_len=${#aa_alt} ;;

        # [nt:]12345:acg>att..
        ?(nt:)$POS:$NTS\>$NTS)      r=${a/#nt:}
                                    nt_pos=${r/%:*} r=${r/#*:} nt_ref=${r/%>*} nt_alt=${a/#*>} ref_len=${#nt_ref} alt_len=${#nt_alt} ;;
        # [aa:]S:345:KL>MN..
        ?(aa:)$CDS:$POS:$AAS\>$AAS) r=${a/#aa:} aa_cds=${r/%:*} r=${r/#$CDS:}
                                    aa_pos=${r/%:*} r=${r/#*:} aa_ref=${r/%>*} aa_alt=${a/#*>} ref_len=${#aa_ref} alt_len=${#aa_alt} ;;
        # [nt:]a..12345c..
        ?(nt:)$NTS$POS$NTS)         r=${a/#nt:}
                                    nt_ref=${r/%$POS$NTS} r=${r/#$NTS} nt_pos=${r/%$NTS} nt_alt=${r/#$POS} ref_len=${#nt_ref} alt_len=${#nt_alt} ;;
        # [aa:]S:Q..12345S..
        ?(aa:)$CDS:$AAS$POS$AAS)    r=${a/#aa:} aa_cds=${r/%:*} r=${r/#$CDS:}
                                    aa_ref=${r/%$POS$AAS} r=${r/#$AAS} aa_pos=${r/%$AAS} aa_alt=${r/#$POS} ref_len=${#aa_ref} alt_len=${#aa_alt} ;;

        # [nt:]12345c..
        ?(nt:)$POS$NTS)             r=${a/#nt:}
                                    nt_ref=${r/%$POS$NTO} r=${r/#$NTO} nt_pos=${r/%$NTO} nt_alt=${r/#$POS} ref_len=${#nt_alt} alt_len=${#nt_alt} ;;
        # [aa:]S:12345S...
        ?(aa:)$CDS:$POS$AAS)        r=${a/#aa:} aa_cds=${r/%:*} r=${r#$CDS:}
                                    aa_ref=${r/%$POS$AAO} r=${r/#$AAO} aa_pos=${r/%$AAO} aa_alt=${r/#$POS} ref_len=${#aa_alt} alt_len=${#aa_alt} ;;

        # Exhausted
        *)  err_msg "failed to parse: $a"; return 1 ;;

        esac ;;

    3|4)
        nt_pos=${args[-3]} nt_ref=${args[-2]} nt_alt=${args[-1]} ref_len=${#nt_ref} alt_len=${#nt_alt}  # VCF format: [label] pos ref alt

        [[ "$nt_pos" =~ [0-9]+ ]] && [[ "$nt_ref" =~ [acgtACGT]+ ]] && [[ "$nt_alt" =~ [acgtACGT]+ ]] || {
            err_msg "invalid values: ${args[@]}"
            return 1;
        } ;;

    *)  err_msg "too many fields: ${#args[@]}"
        return 1 ;;

    esac

    [ -z "$DEBUG" ] || declare -p label nt_pos aa_pos ref_len nt_ref nt_alt aa_cds aa_ref aa_alt
}

# splice_insert generalises the VCF convention for declaring an insert to
# having any number of bases in the ref covering arbitrary ends of the alt.
# Takes ref and alt as $1 and $2, echoes the offset to stdout.
splice_insert() {
    let n=${#1} i=n
    [[ "$1" == "${2:0:n}" ]] || while (( --i >= 0 )); do
        [[ "${1:0:i}" == "${2:0:i}" && "${1:i-n}" == "${2:i-n}" ]] && break
    done
    echo $i
}

# Produce a string of $2-$1 dashes
dashes() { let d=$2-$1; while ((d-- > 0)); do printf -- '-'; done; }

# Takes strings $1 and $3 and separator $2, and returns "$1$2$3" with dash-padded $1 or $2
dash_pad() { printf '%s%s%s%s%s' "$1" "$(dashes ${#1} ${#3})" "$2" "$3" "$(dashes ${#3} ${#1})"; }

# Translate $1 from nt to aa, must be integer number of codons
translate() { debug "translate '$1'"; let j=0; while ((j < ${#1})); do printf "${cod_aa[${1:$j:3}]}"; let j+=3; done; }

### PROCESS DATA ----------------------------------------------------------------------------------

while parse_line; do

    if (( nt_pos )); then

        # Convert to 0-based and check in genome range
        let --nt_pos
        (( nt_pos >= 0 )) && (( nt_pos < ${end_pos[3UTR]} )) || err_exit "out of range position: $nt_pos"

        # Determine inc_len
        let inc_len=alt_len-ref_len
        debug "ref_len = $ref_len; alt_len = $alt_len; inc_len = $inc_len"

        # Look up actual reference at pos
        nts="${genome:$nt_pos:$ref_len}"
        [ -z "$nt_ref" ] && nt_ref="$nts" || [ "$nts" = "$nt_ref" ] ||
            err_exit "reference (${nt_ref}) mismatch with genome: ${nts}"
        debug "nt_ref = $nt_ref"

        # Convert ins to have ref_len=0 and del to have alt_len=0, or error out
        if (( inc_len > 0 )); then
            let ofs=$(splice_insert "$nt_ref" "$nt_alt")
            (( ofs != -1 )) || err_exit "cannot handle mutated insertion: ${nt_ref}>${nt_alt}"
            nt_ref="" nt_alt="${nt_alt:$ofs:$inc_len}"; nt_pos+=ofs alt_len=inc_len ref_len=0
            change="ins:$((nt_pos+1)):${#nt_alt}"
        elif (( inc_len < 0 )); then
            let ofs=$(splice_insert "$nt_alt" "$nt_ref")
            (( ofs != -1 )) || err_exit "cannot handle mutated deletion: ${nt_ref}>${nt_alt}"
            nt_alt="" nt_ref="${nt_ref:$ofs:-$inc_len}"; let nt_pos+=ofs ref_len=-inc_len alt_len=0
            change="del:$((nt_pos+1)):${#nt_ref}"
        elif [[ "$nt_ref" != "$nt_alt" ]]; then
            change="mut:$((nt_pos+1)):${nt_ref}>${nt_alt}"
        else
            change="nt:$((nt_pos+1)):${nt_ref}"
        fi

        # Print optional label column 1, and column 2 canonical change
        printf "${label}${label:+\t}$change\t"

        # Done if no nt change at all
        [[ "$nt_ref" != "$nt_alt" ]] || { printf -- '-\t-\n'; continue; }

        # Make the mutant and slip_mutant genome
        mutant="${genome:0:nt_pos}${nt_alt}${genome:$((nt_pos+ref_len))}"
        slip_mutant="${mutant:0:$((slip_pos+1))}${mutant:$slip_pos}"

        # Collect columns 3 (nt on CDS) and 4 (aa on CDS)
        nt_col=''
        aa_col=''
        for i in "${!reg_ord[@]}"; do

            # Note we know at this point that nt_ref != nt_alt and
            # !ref_len => alt_len (ins) and !alt_len => ref_len (del)
            # else => ref_len == alt_len && both not 0 (mut)

            prd="${reg_ord[$i]}" cds_name="${reg_name[$i]}"
            let beg=${beg_pos[$prd]} end=${end_pos[$prd]} e_nt=end-beg

            # Get positions on this prd
            let s_nt="nt_pos - beg" e_nt_ref="s_nt + ref_len" e_nt_alt="s_nt + alt_len"
            debug "s_nt = $s_nt; e_nt_ref = $e_nt_ref; e_nt_alt = $e_nt_alt"

            # Skip to next if the mutation doesn't affect this prd at all
            (( e_nt_ref >= 0 && nt_pos < end )) || continue

            # Deal with overruns from the left
            (( s_nt >= 0 )) || let s_nt=0
            (( e_nt_alt >= 0 )) || let e_nt_alt=0

            # Deal with overruns at the right
            (( e_nt_ref <= e_nt )) || let e_nt_ref=e_nt  # trim mutate or delete to end
            (( e_nt_alt <= e_nt || inc_len > 0)) || let e_nt_alt=e_nt  # trim mutation to end
            debug "s_nt = $s_nt; e_nt_ref = $e_nt_ref; e_nt_alt = $e_nt_alt"

            # Collect nt_col
            nt_col+="${nt_col:+ }nt:$cds_name:$((s_nt+1)):"
            nt_col+="$(dash_pad "${genome:$((beg+s_nt)):$((e_nt_ref-s_nt))}" '>' "${mutant:$((beg+s_nt)):$((e_nt_alt-s_nt))}")"
            debug "nt_col = $nt_col"

            # Skip 5' and 3' hits and correct the pos for the slippage
            case $prd in
            5UTR|3UTR)
                continue ;;
            ORF1AB|NSP12)
                ((beg + s_nt <= slip_pos)) || ((++s_nt))
                ((beg + e_nt_ref < slip_pos)) || ((++e_nt_ref))
                ((beg + e_nt_alt < slip_pos)) || ((++e_nt_alt))
                declare -n ref_tgt=slip_genome alt_tgt=slip_mutant ;;
            *)
                declare -n ref_tgt=genome alt_tgt=mutant ;;
            esac
            debug "s_nt' = $s_nt; e_nt_ref' = $e_nt_ref; e_nt_alt' = $e_nt_alt"

            # Collect the aa_ref and aa_alt
            let s_aa="s_nt/3"
            let e_aa="((e_nt_ref > s_nt)) ? 1+(e_nt_ref-1)/3 : ((e_nt_alt % 3)) ? 1+s_aa : s_aa"
            aa_ref="$(translate "${ref_tgt:$((beg+3*s_aa)):$((3*(e_aa-s_aa)))}")"
            debug "s_aa = $s_aa; e_aa = $e_aa; aa_ref = $aa_ref"
            let e_aa="((e_nt_alt > s_nt)) ? 1+(e_nt_alt-1)/3 : ((e_nt_ref % 3)) ? 1+s_aa : s_aa"
            aa_alt="$(translate "${alt_tgt:$((beg+3*s_aa)):$((3*(e_aa-s_aa)))}")"
            debug "s_aa = $s_aa; e_aa = $e_aa; aa_alt = $aa_alt"

            # Append the final result
            [ "$aa_ref" = "$aa_alt" ] ||
            aa_col+="${aa_col:+ }$cds_name:$(dash_pad "$aa_ref" $((s_aa+1)) "${aa_alt}")${fs:+ $fs}"

        done

        # Print the NT column
        printf "${nt_col:-"nt:UTR:$((nt_pos+1))"}\t"

        # Add warning when frameshift
        (( inc_len % 3 )) && fs_warn="!frameshift:$((-inc_len%3))" || unset fs_warn

        # Print the AA column
        printf -- "${aa_col:--}${fs_warn:+ }${fs_warn}\n"

    elif (( aa_pos )); then

        let --aa_pos
        [ ${beg_pos[$aa_cds]} ] || err_exit "unknown CDS: $aa_cds"

        # Get the nt position
        let beg=${beg_pos[$aa_cds]}
        let nt_pos="beg + 3*aa_pos"

        # Fiddle the slippage
        declare -n gen_tgt=genome
        case $aa_cds in ORF1AB|NSP12)
            (( nt_pos <= slip_pos )) && declare -n gen_tgt=slip_genome || let --nt_pos ;;
        esac
        debug "nt_pos = $nt_pos"

        # Check that pos is on region
        (( nt_pos >= 0 )) && (( nt_pos < end_pos[$aa_cds] )) || err_exit "out of range aa on $aa_cds: $((aa_pos+1))"

        # Get and check the reference nucleotides
        nts="${gen_tgt:$nt_pos:3*ref_len}"
        aas="$(translate "$nts")"
        debug "nts = $nts; aas = $aas"
        [[ -z "$aa_ref" ]] || [[ "$aas" == "$aa_ref" ]] || err_exit "amino acids at $((aa_pos+1)) are: $aas"
        aa_ref="$aas"

        # Show the output
        [ -z "$label" ] || printf '%s\t' "$label"
        printf '%s:%s\tnt:%d:%s>' "$aa_cds" "$(dash_pad "$aa_ref" $((aa_pos+1)) "$aa_alt")" $((nt_pos+1)) "$nts"
        let i=-1; while (( ++i < alt_len )); do
            printf '('; sep='';
            for cod in "${!cod_aa[@]}"; do [ "${cod_aa[$cod]}" != "${aa_alt:i:1}" ] || { printf "$sep$cod"; sep='|'; }; done
            printf ')'
        done

        printf '\n'
    fi
done

exit 0

# vim: sts=4:sw=4:et:ai:si
